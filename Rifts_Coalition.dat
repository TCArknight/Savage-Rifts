    <eval phase="Setup" priority="5000" index="2"><![CDATA[trustme
var DiffStr as number
DiffStr = tagvalue[vhSRArmStr.?] - #traituser[attrStr]

~ Set strength to Armor Strength if equipped
if (field[grIsEquip].value <> 0) then
   perform hero.assign[Rifts.SuperStrength]
   gizmo.child[modPAStren].field[stackQty].value = DiffStr

   ~Only count Strength Mod 1 time, no matter the Stack
   field[grCurrMods].value -= (DiffStr - 1)
else
   gizmo.child[modPAStren].field[stackQty].value = 1
   endif]]></eval>
    <eval phase="PreTraits" priority="4500"><![CDATA[var myVal as number
myVal = field[vhTough].text

if (field[grIsEquip].value <> 0) then
   perform #traitadjust[trTough,+,myVal ,"Power Armor Toughness"]
   herofield[acDefHead].value += myVal 
   herofield[acDefArms].value += myVal 
   herofield[acDefLegs].value += myVal 
   herofield[acDefTorso].value += myVal 
   endif]]></eval>
    <child entity="LoadOut">
