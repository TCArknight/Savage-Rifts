  <thing id="edgArBkMM" name="Arcane Background: Mind Melter" compset="Edge">
    <fieldval field="shortname" value="Arcane Mind Melter"/>
    <usesource source="SavRiftC"/>
    <tag group="Arcane" tag="MindMelter" name="Mind Melter" abbrev="Mind Melter"/>
