  <thing id="edgArBkTW" name="Arcane Background: Techno-Wizard" compset="Edge">
    <fieldval field="edgCost" value="0"/>
    <fieldval field="shortname" value="Arcane Techno-Wizard"/>
    <usesource source="SavRiftC"/>
