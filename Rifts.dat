    <prereq message="Arcane Background required.">
      <validate><![CDATA[
        if (hero.tagis[Hero.HasArcBack] <> 0) then
          endif
        if (@ispick <> 0) then
          altpick.linkvalid = 0
          endif]]></validate>
      </prereq>
    </thing>
  <thing id="edgSRCPA2" name="Improved Cyber-Psychic Alignment" description="The character can ignore two levels of Strain penalties when using her powers." compset="Edge" summary="Offset 2 level of Strain penalty w/using powers" uniqueness="unique">
