    <bootstrap thing="arcLeyLine"></bootstrap>
    </thing>
  <thing id="grpSRMysti" name="Mystic" description="{b}{i}THE MYSTIC HERO’S JOURNEY (FIVE ROLLS){/b}{/i}\nMystics gain three rolls on any of these tables: {b}Enchanted Items &amp; Mystic Gadgets{/b}, {b}Experience &amp; Wisdom{/b}, {b}Magic &amp; Mysticism{/b}.\n\nThey gain two more rolls on any table except Cybernetics.{/b}." compset="Group" uniqueness="unique">
