  <thing id="grpSRMinMe" name="Mind Melter" description="{b}{i}THE MIND MELTER HERO’S JOURNEY (THREE ROLLS){/i}{/b}\nMind Melters get two rolls on any of the following tables: {b}Enchanted Items &amp; Mystic Gadgets{/b}, {b}Experience &amp; Wisdom{/b}, or {b}Psionics{/b}.\n\nThey also get one additional roll on any table except {b}Cybernetics{/b} or {b}Magic{/b}." compset="Group" uniqueness="unique">
