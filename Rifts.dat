    <usesource source="Deadlands" parent="UserParent" name="Deadlands: Reloaded"/>
    <bootstrap thing="wpSRGTNatW"></bootstrap>
    </thing>
  <thing id="wpSRGTNatW" name="Bite/Claw/Tail Slap" compset="Melee" uniqueness="unique">
