    <bootstrap thing="hinWanted">
      <assignval field="domDomain" value="Saving refugees from Tolkeen War"/>
      </bootstrap>
    </thing>
  <thing id="rpSRInhNat" name="Inherent Nature" description="All Lyn-Srial have the Heroic Hindrance and count as supernaturally good." compset="RacialProp">
    <usesource source="SavRiftC"/>
    <bootstrap thing="hinHeroic"></bootstrap>
    </thing>
  <thing id="rpSRNonSta" name="Non-Standard Physiology" description="Armor purchase/repair is double cost. Can’t use a Glitter Boy suit or other standard humanoid power armor. Suffer a −2 to operate a vehicle or its weapons not refitted for the race. Iconic Framework gear is custom-built." compset="RacialProp">
    <usesource source="SavRiftC"/>
    </thing>
  <thing id="rpSRPoorGr" name="Poor Ground Speed" description="Lyn-Srial have −2&quot; Pace (ground only) and a d4 running die." compset="RacialProp">
    <usesource source="SavRiftC"/>
    </thing>
  <thing id="abSRLRegen" name="Regeneration" description="Lyn-Srial make natural healing rolls once a day and may regenerate lost limbs. If a Lyn-Srial gains a permanent injury, she makes a Vigor roll after recovering from Incapacitation. On success, the injury heals after 1d6 days." compset="RaceAbil" uniqueness="unique">
    <fieldval field="shortname" value="Regen"/>
    <usesource source="SavRiftC"/>
    </thing>
  <thing id="abSRStrong" name="Strong" description="Lyn-Srial start with d6, Strength." compset="RaceAbil" summary="Starting Strength of d6." uniqueness="unique">
    <usesource source="SavRiftC"/>
    <eval phase="PreTraits" priority="5000">#traitcreation[attrStr] += 1
      <before name="Calc trtFinal"/>
      </eval>
    </thing>
  <thing id="abSRAgile" name="Agile" description="Quick-Flex start with a d8 Agility (and a Trait maximum of d12+2)." compset="RaceAbil" summary="Starting Agility of d8." uniqueness="unique">
    <usesource source="SavRiftC"/>
    <eval phase="PreTraits" priority="5000">#traitcreation[attrAgi] += 2
      <before name="Calc trtFinal"/>
      </eval>
    </thing>
  <thing id="rpSRAmbide" name="Ambidexterity" description="Race possesses the Ambidextrous Edge." compset="RacialProp">
    <usesource source="SavRiftC"/>
    <bootstrap thing="edgAmbidex"></bootstrap>
    </thing>
  <thing id="rpSRElusiv" name="Elusive" description="Quick-Flex are difficult to pin down in close quarters, giving them a natural +1 to their Parry rating." compset="RacialProp" summary="Parry is +1" uniqueness="unique">
    <usesource source="SavRiftC"/>
    <eval phase="PreTraits" priority="5000"><![CDATA[
      perform #traitadjust[trParry,+,1,"Cultural Template"]]]></eval>
    </thing>
  <thing id="abSRFast" name="Fast" description="Quick-Flex have +2 to Pace and increase their Running die by one die type (usually a Pace of 8 and d8 Running die)." compset="RaceAbil" summary="Pace +2, Increase Running die by one die type.">
    <usesource source="SavRiftC"/>
    <eval phase="Traits" priority="5500"><![CDATA[perform #traitadjust[trPace,+,2,"Fast"]]]></eval>
    </thing>
  <thing id="rpSRSAtten" name="Short Attention Span" description="Quick-Flex have a difficult time sitting still and focusing on intellectual pursuits. They suffer a −1 to all Smarts rolls." compset="RacialProp" summary="-1 on all Smarts rolls." uniqueness="unique">
    <usesource source="SavRiftC"/>
    </thing>
  <thing id="rpSRSupCon" name="Supreme Confidence" description="All Quick-Flex are inherently Overconfident in addition to any Hindrances chosen by the player." compset="RacialProp">
    <usesource source="SavRiftC"/>
    <bootstrap thing="hinConfide"></bootstrap>
    </thing>
  <thing id="abSRAniEmp" name="Animal Empathy" description="Psi-Stalkers have an inherent empathy with animals. They start with Riding d6 and animals treat them as one of their own. This effect can be problematic if the animal in question is a territorial loner." compset="RaceAbil">
    <usesource source="SavRiftC"/>
    <bootstrap thing="skRiding"></bootstrap>
    <eval phase="PreTraits" priority="5000">#traitcreation[skRiding] += 1
hero.childfound[resSkill].field[resSpent].value -= 1</eval>
    </thing>
  <thing id="abSROutsid" name="Outsider" description="Psi-Stalkers are creepy and off-putting, and make those with magic or psionic abilities especially nervous (for obvious reasons). They are barely more than a slave class in the Coalition and despised in the Federation of Magic. They suffer −2 Charisma with everyone except others of their kind." compset="RaceAbil" summary="−2 Cha when dealing with anyone but other Psi-Stalkers." uniqueness="unique">
    <usesource source="SavRiftC"/>
    <tag group="User" tag="Activation"/>
    <eval phase="PreTraits" priority="5000"><![CDATA[
      if (field[abilActive].value <> 0) then
        perform #traitadjust[trCharisma,-,2,"Outsider"]
        endif]]>
      <before name="Calc trtFinal"/>
      </eval>
    </thing>
  <thing id="rpSRPSPsyS" name="Psychic Sense" description="Psi-Stalkers have the inherent ability to {i}detect arcana{/i}, usable at will. They use the Notice skill to detect the presence and location of any psychic or magic energies, beings, or items. They can also use the Tracking skill to track a specific magical or psionic entity. This sense works to detect anything psychic, magic, or supernatural, including invisible and astral beings. Psychic Sense doesn’t work inside sealed armor or vehicles. Cybernetic implants also cause problems; every point of Strain imposes a −1 penalty when the Psi-Stalker uses his Psychic Sense (the Cyber-Psychic Alignment Edge can negate this penalty). See sidebar on page 61." compset="RacialProp">
    <usesource source="SavRiftC"/>
    </thing>
  <thing id="rpSRPsyVam" name="Psychic Vampire" description="Psi-Stalkers require a diet of PPE or ISP drained from living beings. They gain this using the Draining PPE/ISP rules (page 121), which only works by touch. The Psi-Stalker must cut and draw blood from her intended victim or donor (which must be supernaturally, magically, or psionically act ive to provide sustenance). Psi-Stalkers must consume at least 5 Power Points (PPE or ISP) a day or suffer Fatigue. They starve to death after a week of being Incapacitated by lack of “food.” Each 5 PPE/ISP drained restores one level of Fatigue. Consumed Power Points cannot be used to fuel spells or psionic powers." compset="RacialProp">
    <usesource source="SavRiftC"/>
    </thing>
  <thing id="abSRRacEne" name="Racial Enemy" description="Many Psi-Stalker and Simvan tribes have been at war for years. Meetings between the species start argumentative and get worse. \n\nPsi-Stalkers suffer −4 Charisma with Simvan." compset="RaceAbil" summary="−4 Cha when dealing with Simvan." uniqueness="unique">
    <usesource source="SavRiftC"/>
    <tag group="User" tag="Activation"/>
    <eval phase="PreTraits" priority="5000"><![CDATA[
      if (field[abilActive].value <> 0) then
        perform #traitadjust[trCharisma,-,4,"Racial Enemy"]
        endif]]>
      <before name="Calc trtFinal"/>
      </eval>
    </thing>
  <thing id="edgSRConHu" name="Controlled Hunger" description="A few Psi-Stalkers, especially those who explore the world beyond their tribes, learn to master their hunger. They need to feed less often, and need far less to feel satisfied. They also don’t need to bleed victims; only a touch is required.\n\nA Psi-Stalker with this Edge needs only 3 Power Points every 48 hours to avoid Fatigue levels." compset="Edge" summary="Only require 3 Power Points every 48 hours to avoid Fatigue." uniqueness="unique">
    <usesource source="SavRiftC"/>
    <tag group="MinRank" tag="0"/>
    <tag group="EdgeType" tag="Racial"/>
    <exprreq message="Vigor d8 required."><![CDATA[#trait[attrVig] >= 4]]></exprreq>
    <exprreq message="Spirit d6 required."><![CDATA[#trait[attrSpi] >= 3]]></exprreq>
    <exprreq message="Psi-Stalker race required."><![CDATA[hero.tagis[Race.racSRPsiSt] <> 0]]></exprreq>
    </thing>
  <thing id="edgSRHunte" name="Hunter" description="Some Dog Boys and Psi-Stalkers focus their talents on hunting specific prey. Some choose wielders of the arcane (Ley Line Walkers, Mystics, Necromancers, Shifters, or any character with an Arcane Background using PPE), while others hunt down supernatural monsters (demons, dragons, vampires, Minions of Splugorth, Murder Wraiths, and similar creatures). This Edge can only be taken once; such focus cannot be divided.\n\nHunters gain a +2 on Notice and Tracking rolls where a target of their specialization is concerned, and when they roll a raise on an attack against such targets, they roll a d8 instead of a d6 for bonus damage." compset="Edge" summary="+2 Notice and Tracking rolls against specified target." uniqueness="unique">
    <usesource source="SavRiftC"/>
    <tag group="MinRank" tag="1"/>
    <tag group="EdgeType" tag="Combat" name="Combat" abbrev="Combat"/>
    <tag group="User" tag="NeedDomain"/>
    <tag group="DomainTerm" tag="Creature"/>
    <tag group="User" tag="Activation"/>
    <eval phase="PreTraits" priority="5000"><![CDATA[
      if (field[abilActive].value <> 0) then
        perform #traitroll[skNotice,+,2,"Hunter"]
        perform #traitroll[skTracking,+,2,"Hunter"]
        endif]]></eval>
    <exprreq message="Spirit d6 required."><![CDATA[#trait[attrSpi] >= 3]]></exprreq>
    <exprreq message="Fighting d8 required."><![CDATA[#trait[skFighting] >= 4]]></exprreq>
    <exprreq message="Dog Boy or Psi-Stalker race required."><![CDATA[(hero.tagis[Race.racSRPsiSt] + hero.tagis[Race.racSRDogB] ) <> 0]]></exprreq>
    <exprreq message="Vigor d8 required."><![CDATA[#trait[attrVig] >= 4]]></exprreq>
    </thing>
  <thing id="abSRSAniEm" name="Animal Empathy" description="A Simvan has a psionic connection to animals, granting the Beast Master Edge and Riding d6." compset="RaceAbil">
    <usesource source="SavRiftC"/>
    <bootstrap thing="skRiding"></bootstrap>
    <bootstrap thing="edgBstMast">
      <autotag group="thing" tag="skipprereq"/>
      </bootstrap>
    <eval phase="PreTraits" priority="5000">#traitcreation[skRiding] += 1
hero.childfound[resSkill].field[resSpent].value -= 1</eval>
    </thing>
  <thing id="rpSRSApexP" name="Apex Predator" description="Simvan consider themselves pinnacle predators; the ultimate victory is to defeat and consume an enemy. Simvan have the Bloodthirsty Hindrance and most know their deadly habits." compset="RacialProp">
    <usesource source="SavRiftC"/>
    <bootstrap thing="hinBloodth"></bootstrap>
    </thing>
  <thing id="rpSRSInOIn" name="Instincts Over Intellect" description="Simvan act on their instincts. Intellectual pursuits are not common for them. They suffer a −1 penalty to all Smarts rolls." compset="RacialProp">
    <usesource source="SavRiftC"/>
    </thing>
  <thing id="abSRMonBAf" name="Monstrous Beast Affinity" description="Simvan have a natural affinity for large creatures. When using the beast friend power, they pay half the cost for a creature’s Size (effectively 1 ISP per +1 Size). Additionally, they may take a larger than normal creature with their Beast Master Edge as a mount." compset="RaceAbil">
    <usesource source="SavRiftC"/>
    </thing>
  <thing id="abSRSNaPsi" name="Natural Psionics" description="Simvan start with the Arcane Background (Psionics) Edge. Beast friend must be one of their first chosen powers. They start with 15 ISP instead of 10. If a Simvan chooses an Iconic Framework providing Arcane Background (Psionics), he follows all the normal rules for the Framework except he gains beast friend as a bonus power and gains +5 ISP in addition to what the Framework normally provides." compset="RaceAbil" summary="Arcane Background (Psionics); {i}Beast Friend{/i} powers">
    <usesource source="SavRiftC"/>
    <bootstrap thing="edgArcPsi"></bootstrap>
    <bootstrap thing="powBeastFr"></bootstrap>
    <eval phase="PreTraits" priority="5000">#trkmax[trkPower] += 5</eval>
    </thing>
  <thing id="abSRSRacEn" name="Racial Enemy" description="Many Psi-Stalker and Simvan tribes have been at war for years. Meetings between them start argumentative and get worse. \n\nSimvan suffer −4 Charisma with wild Psi-Stalkers (Coalition Psi-Stalkers don’t like them just because they are D-Bees)." compset="RaceAbil" summary="−4 Cha when dealing with wild Psi-Stalkers." uniqueness="unique">
    <usesource source="SavRiftC"/>
    <tag group="User" tag="Activation"/>
    <eval phase="PreTraits" priority="5000"><![CDATA[
      if (field[abilActive].value <> 0) then
        perform #traitadjust[trCharisma,-,4,"Racial Enemy"]
        endif]]>
      <before name="Calc trtFinal"/>
      </eval>
    </thing>
  <thing id="rpSRTLongA" name="Long Arms" description="The Trimadore have a Reach of 1." compset="RacialProp">
    <usesource source="SavRiftC"/>
    <eval phase="Setup" priority="2000">foreach pick in hero from WeapMelee
        eachpick.field[wpReach].value += 1
        nexteach

      perform hero.child[wpUnarmed].setfocus
      focus.field[wpReach].value += 1</eval>
    </thing>
  <thing id="rpSRMecSav" name="Mechanical Savant" description="Trimadore are born with an innate capacity for working with and repairing machines. They begin with the Mr. Fix It Edge, ignoring any requirements." compset="RacialProp">
    <usesource source="SavRiftC"/>
    <bootstrap thing="edgMrFixIt">
      <autotag group="thing" tag="skipprereq"/>
      </bootstrap>
    </thing>
  <thing id="abSRTecSav" name="Technically Savvy" description="Trimadore love science and technology, studying all they can about such things from birth. Trimadore start with Repair d6 and their choice of one of the following at d6: Knowledge (Computers), Knowledge (Cybernetics), Knowledge (Electronics), or Knowledge (Engineering)." compset="RaceAbil">
    <usesource source="SavRiftC"/>
    <bootstrap thing="skRepair"></bootstrap>
    <bootstrap thing="skKnow">
      <autotag group="Rifts" tag="TKnoSkill"/>
      </bootstrap>
    <eval phase="PreTraits" priority="5000"><![CDATA[var myDom as string

#traitcreation[skRepair] += 1
foreach pick in hero where "component.Skill & Skill.skKnow & Rifts.TKnoSkill"
  myDom = eachpick.field[domDomain].text
  ~if ( compare(myDom,"Computer") ) then
    ~#traitcreation[skKnow] += 1
    ~endif

  eachpick.field[trtCreate].value += 1
  nexteach

hero.childfound[resSkill].field[resSpent].value -= 2]]></eval>
    </thing>
  <thing id="rpSRTecSav" name="Technically Savvy" description="Trimadore love science and technology, studying all they can about such things from birth. Trimadore start with Repair d6 and their choice of one of the following at d6: Knowledge (Computers), Knowledge (Cybernetics), Knowledge (Electronics), or Knowledge (Engineering)." compset="RacialProp">
    <fieldval field="usrLabelAr" value="Know: "/>
    <arrayval field="usrArray" index="0" value="--Choose--"/>
    <arrayval field="usrArray" index="1" value="Computers"/>
    <arrayval field="usrArray" index="2" value="Cybernetics"/>
    <arrayval field="usrArray" index="3" value="Electronics"/>
    <arrayval field="usrArray" index="4" value="Engineering"/>
    <usesource source="SavRiftC"/>
    <bootstrap thing="skRepair"></bootstrap>
    <bootstrap thing="skKnow">
      <autotag group="Rifts" tag="TKnoSkill"/>
      </bootstrap>
    <eval phase="PreTraits" priority="5000"><![CDATA[trustme

var myDom as string
~Repair at D6 for free
#traitcreation[skRepair] += 1
hero.childfound[resSkill].field[resSpent].value -= 1

myDom = field[usrSelect].text
doneif (compare(myDom,"--Choose--") = 0)

foreach pick in hero where "Skill.skKnow & Rifts.TKnoSkill"
  eachpick.field[domDomain].text = myDom
  eachpick.field[trtCreate].value += 1
  hero.childfound[resSkill].field[resSpent].value -= 1
  nexteach]]></eval>
    </thing>
