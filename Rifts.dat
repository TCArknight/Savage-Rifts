if (field[abilActive].value <> 0) then
  perform #traitroll[skNotice,+,2,"Keen Sense of Smell"]
  endif]]></eval>
    </thing>
  <thing id="abSRDBBite" name="Natural Weapons" description="Dog Boys bite for Str+d6 damage." compset="RaceAbil" uniqueness="unique">
