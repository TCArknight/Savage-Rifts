    <bootstrap thing="arcCrazy"></bootstrap>
    </thing>
  <thing id="grpSRCybKn" name="Cyber-Knight" description="{b}{i}THE CYBER-KNIGHT HERO’S JOURNEY (FIVE ROLLS){/b}{/i}\n\nCyber-Knights gain three rolls on any of the following tables: {b}Education, Experience &amp; Wisdom, Psionics,{/b} and {b}Training{/b}.\n\nCyber-Knights gain two rolls on any table except {b}Cybernetics{/b} and {b}Magic &amp; Mysticism.{/b}" compset="Group" uniqueness="unique">
    <usesource source="SavRiftC"/>
    <bootstrap thing="arcCybKnig"></bootstrap>
    </thing>
  <thing id="grpSRGlitt" name="Glitter Boy" description="{b}{i}THE GLITTER BOY HERO’S JOURNEY (FIVE ROLLS){/b}{/i}\n\nGlitter Boys gain three rolls on any of the following tables: {b}Cybernetics, Close Combat Weapons, Ranged Weapons,{/b} and {b}Training.{/b}\n\nGlitter Boys gain two additional rolls on any table, although those dealing with magic or psionics may be of limited utility unless the character expects to spend a lot of time out of his armor." compset="Group" uniqueness="unique">
    <usesource source="SavRiftC"/>
    </thing>
  <thing id="grpSRJuice" name="Juicer" description="{b}{i}THE JUICER HERO’S JOURNEY (FIVE ROLLS){/b}{/i}\nJuicers gain three rolls on any of the following tables: {b}Body Armor, Close Combat Weapons, Ranged Weapons, Training,{/b} and {b}Underworld &amp; Black Ops.{/b}\n\nJuicers gain two additional rolls on any tables except those dealing with magic or psionics." compset="Group" uniqueness="unique">
