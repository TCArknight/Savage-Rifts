    <tag group="EdgeType" tag="Power" name="Power" abbrev="Power"/>
    <tag group="MinRank" tag="1" name="Seasoned" abbrev="Seasoned"/>
    <tag group="User" tag="NeedDomain" name="NeedDomain" abbrev="NeedDomain"/>
    <tag group="DomainTerm" tag="EnergyType" name="Energy Type" abbrev="Energy Type"/>
    <pickreq thing="powTelekin"/>
    <pickreq thing="edgSRMPsi2"/>
    </thing>
  <thing id="wpSRPsiBla" name="Psi-Blade" compset="Melee">
