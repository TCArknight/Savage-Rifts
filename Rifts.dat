    <tag group="EdgeType" tag="Power" name="Power" abbrev="Power"/>
    <tag group="MinRank" tag="1" name="Seasoned" abbrev="Seasoned"/>
    <eval phase="PreTraits" priority="5000"><![CDATA[
      perform #traitroll[skFaith,+,1,"Cyber-Psychic Aligmnent"]
      perform #traitroll[skPsionics,+,1,"Cyber-Psychic Aligmnent"]
      perform #traitroll[skSpellcst,+,1,"Cyber-Psychic Aligmnent"]]]>
