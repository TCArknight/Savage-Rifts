    </thing>
  <thing id="faSRCMMGTR" name="Combat Mage Combat Training" description="A Combat Mage begins play with Fighting d8 and 3 specialied combat edges: Battle Hardened, First Strike, and Extraction." compset="FraAbility">
    <usesource source="SavGPlu"/>
    <bootstrap thing="edgSRBHard">
      <autotag group="thing" tag="skipprereq"/>
      </bootstrap>
    <bootstrap thing="edg1stStk">
      <autotag group="thing" tag="skipprereq"/>
      </bootstrap>
    <bootstrap thing="edgExtract">
      <autotag group="thing" tag="skipprereq"/>
      </bootstrap>
    <bootstrap thing="skFighting"></bootstrap>
    <eval phase="PreTraits" priority="5000"><![CDATA[perform #resspent[resSkill,-,1, "Combat Mage"]
foreach pick in hero where "thingid.skFighting"
   eachpick.field[trtBonus].value += 2
nexteach]]></eval>
    </thing>
  <thing id="faSRTRNCSTR" name="Trained Caster" description="Combat Mages have been highly trained in the use of their spellcasting skill, gaining the Wizard Edge." compset="FraAbility">
