    <bootstrap thing="edgWizard"></bootstrap>
    </thing>
  <thing id="fcSRCMBMGCY" name="Cybernetics" description="Cybernetics are not an option for Combat Mages, as such devices interfere with their powers." compset="FraComp" summary="Unable to use Cybernetics.">
