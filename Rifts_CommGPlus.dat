    <bootstrap thing="skSpellcst"></bootstrap>
    <eval phase="Setup" priority="5000">#traitcreation[trSRPPE] += 20</eval>
    <eval phase="Setup" priority="5000" index="2">#traitcreation[skSpellcst] += 2</eval>
    </thing>
