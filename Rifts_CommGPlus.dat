  <thing id="rpSRDRGINF" name="Dragon Sight - Infravision" description="Dragon Hatchlings have strong infravision.  they halve the penalties for dark lighting against living targets (rounded down)" compset="RacialProp" summary="1/2 lighting penalties when fighting anything radiating heat.">
    <usesource source="SavRiftC"/>
    </thing>
  <thing id="rpSRDRGMGT" name="Dragon Might" description="Even Dragon Hatchlings are incredibly strong, beginning with a Strength of d12+4.  They are also very resilient starting with a d8 Vigor.  Neither attribute has a maximum limit." compset="RacialProp" summary="Dragons are Mighty">
    <eval phase="PreTraits" priority="5000">#traitcreation[attrStr] += 8
#traitcreation[attrVig] += 2

hero.childfound[attrStr].field[trtMaximum].value += 5
hero.childfound[attrVig].field[trtMaximum].value += 5</eval>
    </thing>
  <thing id="rpSRDRGVY" name="Very Young Dragon" compset="RacialProp">
    <usesource source="SavRiftC"/>
    <eval phase="Setup" priority="1000">#resmax[resAttrib] -= 2
#resmax[resSkill] -= 5</eval>
    </thing>
  <thing id="rpSRCYBSHD" name="Shadow of Themselves" description="Borgs don&apos;t choose a race (Borg is their race), but may increase an attribute by one die type or take an Edge (meeting all requirements) to reflect their original race." compset="RacialProp" summary="A hint of who they were">
    <usesource source="SavRiftC"/>
    <eval phase="PreTraits" priority="5000">#resmax[resHinder] += 2</eval>
    </thing>
  <thing id="rpSRCYGRMT" name="Gray Matter" description="Cyborgs only start with two points (instead of the normal five) for attributes and these may only be spent on Smarts and Spirit.  Point from Hindrances and Advances may be used to raise any attribute." compset="RacialProp">
    <usesource source="SavRiftC"/>
    <eval phase="Setup" priority="1000">#resmax[resAttrib] -= 3</eval>
    </thing>
  <thing id="rpSRCYBIHA" name="Inhuman Appearance" description="Combat Cyborgs suffer −2 Charisma when dealing with most beings." compset="RacialProp">
    <usesource source="SavRiftC"/>
    <eval phase="Traits" priority="5000">#traitbonus[trCharisma] -= 2</eval>
    </thing>
  <thing id="rpSRDRGCYB" name="Cybernetics" description="Dragons cannot take cybernetics at any point.  Their bodies simply reject such foreign implants." compset="RacialProp">
    <usesource source="SavRiftC"/>
    </thing>
  <thing id="rpSRDRGFRM" name="Dragon Form Limits" description="In their natral form, dragons cannot wear any armor and can only use vehicular weapons specially adapted for their use at four times the normal cost.  Most other gear can only be used in humanoid form." compset="RacialProp">
    <usesource source="SavRiftC"/>
    </thing>
  <thing id="rpSRDRGOUT" name="Dragon Outsider" description="Dragon Hatchlings understand so little of the world, they inevitably cause socially awkward situations wherever they go, even in humanoid form.  All Dragon Hatchlings have the Outsider Hindrance." compset="RacialProp">
