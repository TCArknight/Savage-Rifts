rolls += hero.tagcount[RiftsHJ.MagMys]
rolls += hero.tagcount[RiftsHJ.Cyber]

validif (rolls = 0)</evalrule>
    </thing>
  <thing id="arcSRPSLNG" name="Psi-Slinger" description="Psi-slingers are minor psionics focused on precognition and telekinesis." compset="Arcane" summary="Psi Slingers unlock their psionic potential">
